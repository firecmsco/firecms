{
  "name": "firecms-app-shared",
  "version": "1.0.0",
  "description": "Shared collections and config for FireCMS app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@firecms/types": "4.0.0-alpha.1"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./collections": {
      "types": "./dist/collections.d.ts",
      "import": "./dist/collections.js"
    }
  }
}

