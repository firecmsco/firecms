{
  "name": "@firecms/custom-backend",
  "version": "1.0.0",
  "description": "Custom PostgreSQL backend for FireCMS with real-time sync",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "generate:schema": "tsx node_modules/@firecms/backend/dist/generate-drizzle-schema.js",
    "generate:schema:watch": "tsx node_modules/@firecms/backend/dist/generate-drizzle-schema.js --watch",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@firecms/backend": "3.0.0-canary.258",
    "app-shared": "1.0.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.6.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "drizzle-kit": "^0.20.6",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
