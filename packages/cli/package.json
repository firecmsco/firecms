{
  "name": "@firecms/cli",
  "version": "3.0.1",
  "description": "A CLI to perform FireCMS operations",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "source": "src/index.ts",
  "bin": "bin/firecms.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf templates/template_cloud/node_modules && rm -rf templates/template_cloud/yarn.lock && vite build && tsc --emitDeclarationOnly -p tsconfig.json",
    "dev": "ts-node src/test.ts",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "execa": "^4.1.0",
    "form-data": "^4.0.5",
    "googleapis": "^166.0.0",
    "inquirer": "12.11.1",
    "json5": "^2.2.3",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "nocache": "^4.0.0",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "pump": "^3.0.3",
    "ws": "^8.18.3",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "templates/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "exclude": [
    "node_modules",
    "template/node_modules"
  ],
  "gitHead": "e249d1f2afcb3784cdf49109c80e34a53e58c670"
}
