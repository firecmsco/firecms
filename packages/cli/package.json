{
  "name": "@firecms/cli",
  "version": "3.0.0-canary.31",
  "description": "A CLI to perform FireCMS operations",
  "packageManager": "yarn@4.1.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "source": "src/index.ts",
  "bin": {
    "firecms": "bin/firecms"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf templates/template_cloud/node_modules && rm -rf templates/template_cloud/yarn.lock && vite build && tsc --emitDeclarationOnly -p tsconfig.json",
    "dev": "ts-node src/test.ts",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "keywords": [
    "cli",
    "create-firecms-app",
    "firecms",
    "firebase",
    "firestore",
    "cms",
    "react",
    "typescript"
  ],
  "author": "firecms.co",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "axios": "^0.28.0",
    "chalk": "^4.1.2",
    "execa": "^4.1.0",
    "form-data": "^4.0.0",
    "googleapis": "^134.0.0",
    "inquirer": "^7.3.3",
    "json5": "^2.2.3",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "nocache": "^4.0.0",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "ws": "^8.16.0",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.3"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "templates/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "exclude": [
    "node_modules",
    "template/node_modules"
  ],
  "gitHead": "ef347a3a787e808176cd79e2ac7568faca37148b"
}
