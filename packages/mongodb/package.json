{
    "name": "@firecms/mongodb",
    "type": "module",
    "version": "4.0.0-alpha.1",
    "description": "MongoDB backend for FireCMS",
    "funding": {
        "url": "https://github.com/sponsors/firecmsco"
    },
    "author": "FireCMS",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/firecmsco/firecms.git",
        "directory": "packages/mongodb"
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/mongodb/src/index.d.ts",
    "source": "src/index.ts",
    "engines": {
        "node": ">=14"
    },
    "keywords": [
        "firebase",
        "cms",
        "admin",
        "admin panel",
        "mongodb",
        "backend",
        "database",
        "firecms"
    ],
    "scripts": {
        "watch": "vite build --watch",
        "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
        "prepublishOnly": "run-s build",
        "test:lint": "eslint \"src/**\" --quiet",
        "test": "jest",
        "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
    },
    "exports": {
        ".": {
            "development": "./src/index.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js",
            "types": "./dist/mongodb/src/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "dependencies": {
        "@firecms/types": "4.0.0-alpha.1",
        "@firecms/core": "4.0.0-alpha.1",
        "mongodb": "^6.12.0",
        "realm-web": "^2.0.0",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@firecms/types": "4.0.0-alpha.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.10.5",
        "@types/ws": "^8.5.10",
        "jest": "^29.7.0",
        "ts-jest": "29.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "mongodb-memory-server": "^10.0.0"
    },
    "peerDependencies": {
        "@firecms/backend": "4.0.0-alpha.1"
    },
    "peerDependenciesMeta": {
        "@firecms/backend": {
            "optional": true
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "gitHead": "9ecf37abf793bd2f2daaaed6f517ee5ee19b01ae"
