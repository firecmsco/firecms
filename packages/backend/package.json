{
  "name": "@firecms/backend",
  "type": "module",
  "version": "4.0.0-alpha.1",
  "description": "PostgreSQL and Drizzle ORM backend for FireCMS",
  "funding": {
    "url": "https://github.com/sponsors/firecmsco"
  },
  "author": "FireCMS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/firecmsco/firecms.git",
    "directory": "packages/backend"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "firebase",
    "cms",
    "admin",
    "admin panel",
    "postgresql",
    "drizzle",
    "backend",
    "database",
    "firecms"
  ],
  "scripts": {
    "watch": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "prepublishOnly": "run-s build",
    "test:lint": "eslint \"src/**\" --quiet",
    "test": "jest",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@firecms/common": "4.0.0-alpha.1",
    "@firecms/types": "4.0.0-alpha.1",
    "drizzle-orm": "^0.44.4",
    "pg": "^8.11.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "graphql": "^16.8.1",
    "graphql-http": "^1.22.0",
    "jsonwebtoken": "^9.0.0",
    "google-auth-library": "^9.0.0",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "@firecms/common": "4.0.0-alpha.1",
    "@firecms/types": "4.0.0-alpha.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/ws": "^8.5.10",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "drizzle-kit": "^0.31.4",
    "jest": "^29.7.0",
    "ts-jest": "29.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "@types/nodemailer": "^6.4.0"
  }
}