{
  "name": "@firecms/datatalk",
  "version": "4.0.0-alpha.1",
  "description": "Talk to your Firestore data",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@firecms/collection_editor_firebase": "4.0.0-alpha.1",
    "@firecms/core": "4.0.0-alpha.1",
    "@firecms/types": "4.0.0-alpha.1",
    "@firecms/data_export": "4.0.0-alpha.1",
    "@firecms/data_import": "4.0.0-alpha.1",
    "@firecms/data_import_export": "4.0.0-alpha.1",
    "@firecms/firebase": "4.0.0-alpha.1",
    "@firecms/formex": "4.0.0-alpha.1",
    "@firecms/schema_inference": "4.0.0-alpha.1",
    "@firecms/ui": "4.0.0-alpha.1",
    "@monaco-editor/react": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "20.17.14",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "typescript": "^5.8.3",
    "vite": "^5.4.17"
  },
  "gitHead": "946e1f4f5695190e903af3a71dd7111accfc1efa"
}
