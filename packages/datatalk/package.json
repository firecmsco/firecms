{
  "name": "@firecms/datatalk",
  "version": "3.1.0",
  "description": "Talk to your Firestore data",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@firecms/collection_editor_firebase": "^3.1.0",
    "@firecms/core": "^3.1.0",
    "@firecms/data_export": "^3.1.0",
    "@firecms/data_import": "^3.1.0",
    "@firecms/data_import_export": "^3.1.0",
    "@firecms/firebase": "^3.1.0",
    "@firecms/formex": "^3.1.0",
    "@firecms/schema_inference": "^3.1.0",
    "@firecms/ui": "^3.1.0",
    "@monaco-editor/react": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "20.19.17",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "gitHead": "9ecf37abf793bd2f2daaaed6f517ee5ee19b01ae"
}
