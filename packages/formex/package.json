{
  "name": "@firecms/formex",
  "type": "module",
  "version": "3.0.0-canary.133",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "firebase",
    "cms",
    "admin",
    "admin panel",
    "firebase panel",
    "firestore",
    "headless",
    "headless cms",
    "content manager"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "react-fast-compare": "^3.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f",
    "test": "jest"
  },
  "files": [
    "dist",
    "src",
    "tailwind.config.js",
    "bin"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "gitHead": "0c46cee41d666c60817c9f4f0447f78fcfa07ddf"
}
