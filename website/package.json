{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "run-s copyChangelog && docusaurus start",
    "build": "REACT_GENERATE_API=true && NODE_OPTIONS=--max-old-space-size=24048 && run-s genLLMsTxt && run-s copyChangelog && docusaurus build",
    "preview": "run-s build && docusaurus serve",
    "genLLMsTxt": "node scripts/generate_llms_txt.js",
    "genAPI": "typedoc --plugin typedoc-plugin-markdown --hideGenerator --hideInPageTOC --out docs/api",
    "copyChangelog": "echo '---' > ./docs/CHANGELOG.md && echo 'id: changelog' >> ./docs/CHANGELOG.md && echo 'title: Changelog' >> ./docs/CHANGELOG.md && echo '---' >> ./docs/CHANGELOG.md && cat ../CHANGELOG.md >> ./docs/CHANGELOG.md ",
    "swizzle": "docusaurus swizzle --danger",
    "deploy": "run-s build && firebase deploy --only hosting",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@docsearch/js": "3.7.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-google-gtag": "3.7.0",
    "@docusaurus/plugin-google-tag-manager": "^3.8.1",
    "@docusaurus/plugin-sitemap": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-search-algolia": "3.7.0",
    "@firecms/ui": "workspace:*",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-responsive-masonry": "^2.3.0",
    "react-type-animation": "^2.1.2",
    "tailwindcss": "^3.4.7",
    "three": "^0.169.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@docusaurus/faster": "^3.7.0",
    "@docusaurus/module-type-aliases": "latest",
    "@docusaurus/types": "latest",
    "@firecms/cloud": "workspace:*",
    "@firecms/collection_editor": "workspace:*",
    "@firecms/core": "workspace:*",
    "@firecms/data_enhancement": "workspace:*",
    "@firecms/data_export": "workspace:*",
    "@firecms/data_import": "workspace:*",
    "@firecms/firebase": "workspace:*",
    "@firecms/formex": "workspace:*",
    "@firecms/neat": "0.4.0",
    "@firecms/schema_inference": "workspace:*",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.3.11",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "docusaurus-plugin-sass": "^0.2.5",
    "docusaurus-plugin-typedoc": "^1.2.2",
    "docusaurus-plugin-typedoc-api": "^4.4.0",
    "dotenv": "^16.4.5",
    "esbuild-loader": "^4.2.2",
    "fontaine": "^0.5.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "openai": "^4.55.0",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "react-compiler-webpack": "^0.1.2",
    "sass": "^1.77.8",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.2.3",
    "typescript": "^5.7.3"
  }
}
