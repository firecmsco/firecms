# GCP Marketplace Provider ID (assigned during partner onboarding)
GCP_MARKETPLACE_PROVIDER_ID=firecms

# Pub/Sub topic for Marketplace events (provided by Google during onboarding)
GCP_MARKETPLACE_PUBSUB_TOPIC=cloud-commerce-procurement

# Your application URL for redirects
APP_URL=https://firecms.co

# Cloud Functions region
FUNCTIONS_LOCATION=us-central1

# Default VM zone for Typesense
VM_ZONE=us-central1-a

# Firestore collections to index (* for all)
COLLECTIONS_TO_INDEX=*
